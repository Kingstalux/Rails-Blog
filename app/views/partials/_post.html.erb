    <div class="post">
        <h3><%=post.title%></h3>
        <p><%=post.text%></p>

        <div class="spam">
            <p>Comments:  <%=post.commentsCounter%></p>
            <p>Likes:  <%=post.likesCounter%></p>
            </div>
            <%= button_to "Delete", user_post_path(@user.id, post.id), :method => :delete %>
    </div>